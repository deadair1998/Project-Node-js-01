

		<% include templates/header %> 
		<!-- lead -->
		<a class="ish-row ish-fullrow ish-lead" href="portfolio-detail1.html">
			<span class="ish-lead__caption ish-icon-up">Featured Portfolio</span>
			<div class="ish-lead__image">
				<img src="images/header.jpg" alt="work in silent">
			</div>
			<h1 class="ish-lead__headline">
				<span class="ish-lead__headline-inner">Workin<br>Silent</span>
			</h1>
		</a>
		<!-- lead end -->



		<!-- content -->
		<div class="ish-row ish-content ish-portfolio ish-macy">

      <!-- main content -->
			<ul class="ish-portfolio-item ish-portfolio-filter">
				<li class="ish-portfolio-filter__item ish-portfolio-filter--title"><h5>Filter:</h5></li>
				<li class="ish-portfolio-filter__item"><a class="ish-portfolio-filter__link ish-portfolio-filter__link--active" href="#all">All</a></li>
				<% data.product.forEach(element => { %>
				 	<li class="ish-portfolio-filter__item"><a class="ish-portfolio-filter__link" href="#<%= element.categories %> "><%= element.categories %> </a></li>
				<% }) %>
				
				
			</ul>
			<% data.product.forEach(function(value){ %>
			 	<a class="ish-portfolio-item ish--tc13 ish-portfolio-<%= value.categories %> " href="/<%= value.seo%>.<%= value.id %> ">
				<div class="ish-portfolio-item__image-container">
					<img class="ish-portfolio-item__image" src="<%= value.imgBackground  %>"  alt="business cards">
				</div>
				<div class="ish-portfolio-item__desc">
					<span class="ish-portfolio-item__title"><%= value.title %> </span>
					<span class="ish-portfolio-item__subtitle"><%= value.categories %> </span>
				</div>
			</a>
			<% }) %>								
		</div>
			
     
	<% include templates/footer %> 

		